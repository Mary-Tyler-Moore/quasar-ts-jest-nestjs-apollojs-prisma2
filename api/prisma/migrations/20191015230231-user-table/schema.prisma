datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator photon {
  provider = "photonjs"
}

model Book {
  id    String  @default(cuid()) @id @unique
  title String
  description  String?
}

model User {
  id String @default(cuid()) @id @unique
  username String @unique
  emailAddress String @unique
  password String
}
